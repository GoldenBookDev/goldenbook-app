{
  "expo": {
    "name": "Goldenbook",
    "slug": "goldenbook",
    "scheme": "com.bwebstudio.goldenbook",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Goldenbook necesita acceder a tu ubicaci칩n para mostrarte lugares cercanos.",
          "locationAlwaysPermission": "Goldenbook usa tu ubicaci칩n para mostrarte lugares cercanos incluso cuando la app est치 en segundo plano.",
          "locationWhenInUsePermission": "Goldenbook usa tu ubicaci칩n para mostrarte lugares cercanos."
        }
      ],
      [
        "@react-native-firebase/app"
      ],
      [
        "@react-native-firebase/auth"
      ],
      [
        "react-native-app-auth",
        {
          "android": {
            "serviceConfigurationAndroid": {
              "googleConfiguration": {
                "redirectUrl": "com.bwebstudio.goldenbook:/oauth2redirect/google"
              }
            }
          },
          "ios": {
            "serviceConfigurationIOS": {
              "googleConfiguration": {
                "redirectUrl": "com.bwebstudio.goldenbook:/oauth2redirect/google"
              }
            }
          }
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.bwebstudio.goldenbook",
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true
    },
    "android": {
      "package": "com.bwebstudio.goldenbook",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "134f9bf5-71ef-42f0-bef9-789e42476be1"
      }
    },
    "owner": "goldenbook"
  }
}